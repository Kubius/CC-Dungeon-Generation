# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# 2-space indentation for all files, uncomment the following line if you start using the .lua extension
# [*.lua]
indent_style = space
indent_size = 2
